{
  "name": "uistd/async",
  "description": "UI Service 框架异步操作组件",
  "keywords": [
    "ui service",
    "uistd",
    "async"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder and project lead"
    }
